{
  "name": "@iconizza/vue2",
  "version": "0.0.0",
  "description": "Iconizza icon component for Vue 2.",
  "author": "Dennis Ollhoff",
  "license": "MIT",
  "funding": "https://github.com/sponsors/nyxb",
  "homepage": "https://iconizza.design/",
  "repository": {
    "type": "git",
    "url": "https://github.com/iconizza/iconizza.git",
    "directory": "components/vue2"
  },
  "bugs": "https://github.com/iconizza/iconizza/issues",
  "exports": {
    "./*": "./*",
    ".": {
      "types": "./dist/iconizza.d.ts",
      "import": "./dist/iconizza.mjs",
      "default": "./dist/iconizza.js"
    },
    "./offline": {
      "types": "./dist/offline.d.ts",
      "import": "./dist/offline.mjs",
      "default": "./dist/offline.js"
    },
    "./dist/offline": {
      "types": "./dist/offline.d.ts",
      "import": "./dist/offline.mjs",
      "default": "./dist/offline.js"
    }
  },
  "main": "dist/iconizza.js",
  "module": "dist/iconizza.mjs",
  "types": "dist/iconizza.d.ts",
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "build": "node build",
    "build:lib": "tsc -b",
    "build:dist": "rollup -c rollup.config.mjs",
    "prebuild:api": "api-extractor run --local --verbose --config api-extractor.offline.json",
    "build:api": "api-extractor run --local --verbose --config api-extractor.iconizza.json",
    "test": "jest --runInBand"
  },
  "peerDependencies": {
    "vue": "2.x"
  },
  "dependencies": {
    "@iconizza/types": "^0.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@iconizza/core": "^0.0.1",
    "@iconizza/utils": "^0.0.1",
    "@microsoft/api-extractor": "^7.35.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/jest": "^29.5.2",
    "@vue/test-utils": "^1.3.6",
    "@vue/vue2-jest": "^29.2.4",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rollup": "^2.79.1",
    "typescript": "^5.1.3",
    "vue": "^2.7.14",
    "vue-template-compiler": "^2.7.14"
  }
}
